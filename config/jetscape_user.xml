<?xml version="1.0"?>

<jetscape>
  
  <nEvents> 200 </nEvents>
  
  <JetScapeWriterAscii> on </JetScapeWriterAscii>

  <!-- Inital State Module  -->
  <IS>
    <Trento use_module="user_defined"> </Trento>
  </IS>
  
  <!-- Hard Process -->
  <Hard>
    <PythiaGun>
      <pTHatMin>50</pTHatMin>
      <pTHatMax>70</pTHatMax>
      <eCM>5020</eCM>
    </PythiaGun>
  </Hard>
  
  <!--Preequilibrium Dynamics Module -->
  <Preequilibrium>
    <FreestreamMilne> </FreestreamMilne>
  </Preequilibrium>
  
  <!-- Hydro  Module  -->
  <Hydro>
    <Brick bjorken_expansion_on="false" start_time="0.6"> </Brick>
  </Hydro>
  
  <!--Eloss Modules -->
  <Eloss>
    <Matter> </Matter>
    <LBT> </LBT>
  </Eloss>
  
  <!-- Jet Hadronization Module -->
  <JetHadronization>
    <name>colored</name>
  </JetHadronization>
  
  <!-- Particlization Module  -->
  <SoftParticlization>
    <iSS> </iSS>
  </SoftParticlization>
  
</jetscape>
