<?xml version="1.0"?>

<jetscape>
  
  <nEvents> 1000 </nEvents>
  
  <JetScapeWriterHepMC> on </JetScapeWriterHepMC>

  <Random>
    <seed>0</seed>
  </Random>

  <!-- Hard Process -->
  <Hard>
    <PythiaGun>
      <pTHatMin>235</pTHatMin>
      <pTHatMax>1000</pTHatMax>
      <eCM>2760</eCM>
    </PythiaGun>
  </Hard>
  
  <!--Eloss Modules -->
  <Eloss>
    <Matter> 
      <Q0> 1.0 </Q0>
      <in_vac> 1 </in_vac>
      <vir_factor> 0.25 </vir_factor>
      <qhat0> 0.0 </qhat0>
      <alphas> 0.25 </alphas>      
    </Matter>
  </Eloss>
  
  <!-- Jet Hadronization Module -->
  <JetHadronization>
    <name>colorless</name>
  </JetHadronization>
  
</jetscape>
