<?xml version="1.0"?>

<jetscape>

  <nEvents> 200 </nEvents>
  
  <!-- Jetscape Writer -->
  <JetScapeWriterAscii> on </JetScapeWriterAscii>

  <!-- Inital State Module  -->
  <IS>
    <Trento use_module="user_defined"> </Trento>
  </IS>

  <!-- Hard Process -->
  <Hard>
    <PGun>
      <pT>50</pT>
    </PGun>
  </Hard>

  <!--Preequilibrium Dynamics Module -->
  <Preequilibrium>
    <FreestreamMilne> </FreestreamMilne>
  </Preequilibrium>

  <!-- Hydro  Module  -->
  <Hydro>
    <Brick bjorken_expansion_on="false" start_time="0.6"> </Brick>
  </Hydro>
  
  <!--Eloss Modules -->
  <Eloss>
    <Matter> </Matter>
    <AdSCFT> </AdSCFT>
  </Eloss>
  
  <!-- Jet Hadronization Module -->
  <JetHadronization>
    <name>colored</name>
  </JetHadronization>

  <!-- Particlization Module  -->
  <SoftParticlization>
    <iSS> </iSS>
  </SoftParticlization>
  
  <!-- Hadronic Afterburner  -->
  <Afterburner>
    <SMASH> </SMASH>
  </Afterburner>

</jetscape>
